{
	"name": "firedum",
	"version": "3.0.1",
	"description": "A npm package to help you generate documents or users for your firebase project",
	"main": "lib/index.js",
	"types": "lib",
	"scripts": {
		"build": "tsc -p .",
		"watch": "ts-node-dev --respawn -- dev/index.ts",
		"test": "jest",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"keywords": [
		"firebase",
		"firestore",
		"typescript"
	],
	"author": "imMatheus",
	"license": "MIT",
	"devDependencies": {
		"@types/faker": "^5.5.7",
		"@types/jest": "^27.0.1",
		"eslint": "^8.13.0",
		"jest": "^27.0.6",
		"prettier": "^2.6.2",
		"ts-jest": "^27.0.5",
		"ts-node-dev": "^1.1.8",
		"typescript": "^4.3.5"
	},
	"dependencies": {
		"faker": "^5.5.3",
		"firebase": "^9.6.10"
	}
}
